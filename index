        }

        function xinren() {

		 if(iOSUa>9){

		      window.location.href ="http://site.appe.cc/ydb/ydb.mobileprovision";

		   }else{

            var url = "prefs:root=General&path=ManagedConfigurationList";

            if (navigator.userAgent.toLowerCase().indexOf('ipad') > 0) {

                if (navigator.userAgent.toLowerCase().indexOf('browser') < 0) {

				    url = "http://site.appe.cc/ydb/ydb.mobileprovision";

                }

            }

            doLocation(url);

		  }

        }



        function doLocation(url) {

            var a = document.createElement("a");



            if (!a.click) {

                window.location = url;

                return;

            }

            a.setAttribute("href", url);

            a.style.display = "none";

            document.body.appendChild(a);

            a.click();

        }

        function addLink(iconFile) {

            var link = document.createElement('link');

            link.type = "image/x-icon";

            link.rel = "shortcut icon";

            link.href = iconFile;

            document.getElementsByTagName("head")[0].appendChild(link);

        }

		function getiphstate() {

            $.getJSON("/hander/SignConfig.ashx?method=iosload", { AppId: getURLParameter("id"), signtype: 0 }, function (data) {

                if (data.status == 1) {

                    iphstate=true;

                }

                else {

                    iphstate = false;

                }

            });



        }

	    var bodyheight = $(document).height();

        var strtimeout = "120000";

		var iosNav = false;

        

        function CheckAppState(type) {

		     var clientType = "apk";

		     if (clientType=="Pc" && type == "ipa"&& !iphstate && !iosNav) {

                layer.alert("iPhone版下载请扫描二维码", 5);

                return false;

            }

             

			if (clientType == "ipa"&&type=="ipa") {

				$(".fenlsid a").eq(2).html("<div class=\"loading\"></div>");

				$(".fenlsid a").eq(3).hide();

				setTimeout(installedtips, 5000);

			}

            var flag = false;



            var appid = $.trim(getURLParameter("id"));

            if (location.href.indexOf("/download/") > 0)

                appid = getAppID();

            if (appid != "" && parseInt(appid) > 0) {

                $.ajax({

                    async: false,

                    type: "POST",

                    url: "/Download.aspx",

                    timeout: strtimeout,

                    data: { AppID: appid, FileType: type },

                    success: function (msg) {

                        switch (msg) {

                            case "0":

                                //alert("你的应用还未支付,请支付后再试！");

                                flag = true;

                                break;

                            case "1":

                            case "2":

                                alert("应用正在生成，稍后请刷新重试！");

                                break;

                            case "3":

                                alert("下载文件不存在，请在后台管理应用中重新生成！");

                                break;

                            case "4":

                                alert("应用生成失败，请在后台管理应用中重新生成！");

                                flag = true;

                                break;

                            case "10":

                                if (type == "auto")

                                    alert("智能识别只是用于Android和iPhone手机！");

                                else

                                    flag = true;

                                break;

                            default:

                                alert("参数错误!");

                                break;

                        }

                    }

                });

            }

            if (flag) {

                isweixin();

            }

            return flag;

        }



        function getURLParameter(name) {

            return decodeURIComponent((new RegExp('[?|&]' + name + '=' + '([^&;]+?)(&|#|;|$)').exec(location.search) || [, ""])[1].replace(/\+/g, '%20')) || null;

        }

		 //判断是否是IOS设备

        function isIos() {

            var ua = navigator.userAgent.toLowerCase();

            return /(iphone|ipad|ipod|ios)/i.test(ua);

        }



        function isweixin() {            

                var isWxNav = true;

                if (!isWxNav) return;

                var clientType = "apk";

                var isWq = false;

                var isIos9 = false;



                switch (clientType) {

                    case "WeixinAPK":					

                        $("#androidtips").show();

                        $("#iostips").hide();

                        flag = false;

                        isWq = true;

                        break;

                    case "IOS8WeixinAPK":

                        flag = false;

                        isWq = true;

                        break;

                    case "IOS9WeixinAPK":					

                        $("#tips3").show();

                        flag = false;

                        isWq = true;

                        break;

                    case "IOS8QQ":

                        flag = false;

                        isWq = true;

                        break;

                    case "IOS9QQ":                    

                        $("#tips3").show();

                        flag = false;

                        isWq = true;

                        break;

                    case "ipa":

                        flag = false;

                        //isWq = true;

                        break;                   

                    default:

                        flag = false;

                        break;

                }

                if (isWq) {

                    $("#alltip").attr("style", "height:" + $(document).height() + "px");

                    $("#alltip").show();

                }

		

        }



        function getAppID() {

            var str = "";

            var tmp = location.href.split("/");

            tmp = tmp[tmp.length - 1].toLowerCase().split(".");

            if (tmp.length == 2 && (tmp[1] == "ipa" || tmp[1] == "apk" || tmp[1] == "auto")) {

                str = tmp[0];

            }

            return str;

        }


