		function installedtips() {

             $(".fenlsid a").eq(2).html("正在安装，请按Home键在桌面查看").attr("class", "installed");

		    var clientType = "apk";

			

            if(isIos()){

                var YundabaoUA =  navigator.userAgent.toLowerCase();//获取当前的useragent

                YundabaoUA=YundabaoUA.substring(YundabaoUA.indexOf("os"));

                var YundabaoUAs = new Array(); //定义一数组 

                YundabaoUAs = YundabaoUA.split(" "); //字符以空格分割成数组

                var _iOSUa=YundabaoUAs[1];

                _iOSUa=_iOSUa.split("_")[0];

                iOSUa=parseInt(_iOSUa);

            }

		    if (isIos() && parseInt(iOSUa)>8 && (clientType == "IOS9QQ" || clientType == "ipa")) {

